<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/Banshee/NotificationAreaIcon/Enabled</key>
      <applyto>/apps/Banshee/NotificationAreaIcon/Enabled</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Enable the Notification Area Icon plugin</short>
        <long></long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/Banshee/NotificationAreaIcon/ShowNotifications</key>
      <applyto>/apps/Banshee/NotificationAreaIcon/ShowNotifications</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Show notification bubbles when the song changes</short>
        <long></long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/Banshee/NotificationAreaIcon/notify_on_close</key>
      <applyto>/apps/Banshee/NotificationAreaIcon/notify_on_close</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Notify that application was closed</short>
        <long>Notify that the application was closed to the notification area and is still running.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/Banshee/NotificationAreaIcon/quit_on_close</key>
      <applyto>/apps/Banshee/NotificationAreaIcon/quit_on_close</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Quit instead of close</short>
        <long>Terminate the application instance instead of closing to the notification area</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
