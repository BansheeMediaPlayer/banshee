<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/banshee/plugins/daap/enabled</key>
      <applyto>/apps/banshee/plugins/daap/enabled</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Plugin enabled</short>
        <long>DAAP plugin enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/daap/server_enabled</key>
      <applyto>/apps/banshee/plugins/daap/server_enabled</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Share server enabled</short>
        <long>Share local music with others</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/daap/share_name</key>
      <applyto>/apps/banshee/plugins/daap/share_name</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default>Banshee Music Share</default>
      <locale name="C">
        <short>Share name</short>
        <long>Music share name</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
