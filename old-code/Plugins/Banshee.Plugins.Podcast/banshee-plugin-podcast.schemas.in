<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/banshee/plugins/podcast/enabled</key>
      <applyto>/apps/banshee/plugins/podcast/enabled</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Plugin enabled</short>
        <long>Podcast plugin enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/podcast/podcast_library</key>
      <applyto>/apps/banshee/plugins/podcast/podcast_library</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Podcast library location</short>
        <long>Root directory for the podcast plugin to store downloaded files</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/podcast/ui/columns/podcast_date_column</key>
      <applyto>/apps/banshee/plugins/podcast/ui/columns/podcast_date_column</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>100</default>
      <locale name="C">
        <short>Podcast date column</short>
        <long>Position of the podcast date playlist column</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/podcast/ui/columns/podcast_feed_column</key>
      <applyto>/apps/banshee/plugins/podcast/ui/columns/podcast_feed_column</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>100</default>
      <locale name="C">
        <short>Podcast feed column</short>
        <long>Position of the podcast feed playlist column</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/podcast/ui/columns/podcast_title_column</key>
      <applyto>/apps/banshee/plugins/podcast/ui/columns/podcast_title_column</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>100</default>
      <locale name="C">
        <short>Podcast title column</short>
        <long>Position of the podcast title playlist column</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/podcast/ui/playlist_separator_position</key>
      <applyto>/apps/banshee/plugins/podcast/ui/playlist_separator_position</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>300</default>
      <locale name="C">
        <short>Playlist separator position</short>
        <long>Position of the separator located between the feed and podcast views</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
