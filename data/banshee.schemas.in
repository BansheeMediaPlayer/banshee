<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/Banshee/PlayerEngine</key>
      <applyto>/apps/Banshee/PlayerEngine</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default>helix</default>
      <locale name="C">
        <short>Player Engine Name</short>
        <long>Name of media playback engine.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/Banshee/SourceViewWidth</key>
      <applyto>/apps/Banshee/SourceViewWidth</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>150</default>
      <locale name="C">
        <short>Source View Width</short>
        <long>Width of Source View Comlumn.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/Banshee/WindowWidth</key>
      <applyto>/apps/Banshee/WindowWidth</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>800</default>
      <locale name="C">
        <short>Window Width</short>
        <long>Width of Main Player Window.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/Banshee/WindowHeight</key>
      <applyto>/apps/Banshee/WindowHeight</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>600</default>
      <locale name="C">
        <short>Window Height</short>
        <long>Height of Main Player Window.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/Banshee/WindowX</key>
      <applyto>/apps/Banshee/WindowX</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Window Position X</short>
        <long>Pixel position of Main Player Window on the X Axis</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/Banshee/WindowY</key>
      <applyto>/apps/Banshee/WindowY</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Window Position Y</short>
        <long>Pixel position of Main Player Window on the Y Axis</long>
      </locale>
    </schema>
	<schema>
	  <key>/schemas/apps/Banshee/Volume</key>
	  <applyto>/apps/Banshee/Volume</applyto>
	  <owner>banshee</owner>
	  <type>int</type>
	  <default>80</default>
	  <locale name="C">
	  	<short>Volume Output Level</short>
		<long>Volume of playback relative to mixer output (except on Helix, which sets volume directly on PCM</long>
      </locale>
	</schema>
	<schema>
	  <key>/schemas/apps/Banshee/FileNamePattern</key>
	  <applyto>/apps/Banshee/FileNamePattern</applyto>
	  <owner>banshee</owner>
	  <type>string</type>
	  <default>%artist%/%album%/%track_number%. %title%</default>
	  <locale name="C">
	    <short>File Name Pattern</short>
		<long>Format for creating filenames/directories for ripped tracks, imported music, etc. Format is relative to the Banshee music directory... do not form an absolute path! Tokens: %artist%, %album%, %title%, %track_number%, %track_count%, %track_number_nz% (No prefixed zero), %track_count_nz% (No prefixed zero).</long>
      </locale>
	</schema>
  </schemalist>
</gconfschemafile>

